   "source": [
    "import math\n",
    "c = 3e8 # wavelength\n",
    "f = 9e8 # carrier frequency\n",
    "D = 1 #Franhofer distance\n",
    "Lamda = c/f\n",
    "print(f\"Lamda is: {Lamda:.2f}\")\n",
    "Df = 2*(D**2)/Lamda\n",
    "Path_Loss = - 10 * math.log10((Lamda**2)/(((4*math.pi)**2)*(Df**2)))\n",
    "print(f\"Path loss of PL(dB): {Path_Loss:.2f}\")"
   ]
